configure terminal
    ipv6 unicast-routing
    interface FastEthernet 0/0
        ipv6 address 2001:192:168:5::6/64
        ipv6 rip ripng enable
        no shutdown
        exit
    interface FastEthernet 0/1
        ipv6 address 2001:192:168:9::6/64
