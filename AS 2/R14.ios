configure terminal
    ipv6 unicast-routing
    ipv6 router ospf 1
        router-id 14.14.14.14
        exit
    interface FastEthernet 0/0
        ipv6 address 2002:192:168:0::14/64
        ipv6 ospf 1 area 0
        no shutdown
        exit
    interface FastEthernet 0/1
        ipv6 address 2002:192:168:1::14/64
        ipv6 ospf 1 area 0
        no shutdown
        end
